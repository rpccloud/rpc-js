  },
  "homepage": "https://github.com/rpccloud/web#readme",
  "devDependencies": {
    "@types/jest": "26.0.9",
    "@types/ws": "7.2.6",
    "jest": "26.2.2",
    "ts-jest": "26.1.4",
    "tslint": "6.1.3",
    "typescript": "3.9.7",
    "ws": "7.4.6"
  }
